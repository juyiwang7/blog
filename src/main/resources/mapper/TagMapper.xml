<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hdu.blog.mapper.TagMapper">
    <select id="listTag" resultType="com.hdu.blog.entity.Tag">
        select * from tag
    </select>

    <select id="getTag" parameterType="int" resultType="com.hdu.blog.entity.Tag">
        select tag_id,tag_name,user_id,create_time,update_time from tag where tag_id=#{id}
    </select>
</mapper>